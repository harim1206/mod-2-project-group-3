<%= link_to "Add New Memory", new_memory_path %><br/>
<%= link_to "View All Memories", memories_path %><br/>

<% if @memories %>
  <% @memories.each do |memory| %>
    <%if memory.image_url.strip != "" %>
      <p><%= link_to image_tag(memory.image_url), memory %></p>
    <% end %>
  <% end %>
<% end %>
