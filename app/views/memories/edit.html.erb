<% if !current_user.family.memories.include?(@memory) %>
  <%= render 'noaccess' %>
<% else %>
  <h2>Edit Memory</h2><br/>

  <%= render 'form' %><br/>

  <div class = "formmemories" id="deletebtn">

    <%= form_tag memory_path(@memory), method: 'delete' do %>
     <%= submit_tag 'Delete Memory', class: "btn btn-default"%>
    <% end %>

  </div>
<% end %>
